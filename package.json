{
  "name": "@molgenis/molgenis-js-library-example",
  "version": "0.0.1",
  "description": "A javascript example library for MOLGENIS",
  "main": "dist/molgenis-js-library-example.es.js",
  "scripts": {
    "lint": "eslint src test",
    "unit": "eslint src test; babel-node ./node_modules/istanbul/lib/cli.js cover --dir test/unit/coverage ./node_modules/mocha/bin/_mocha test/unit/specs/**/*.js",
    "debug": "eslint src test; mocha --compilers js:babel-register --debug-brk",
    "unit:watch": "eslint src test; mocha --compilers js:babel-register --reporter min --watch",
    "build": "eslint src test; node build/build",
    "preversion": "yarn install; yarn test; yarn build"
  },
  "publishConfig": {
    "access": "public",
    "registry": "http://registry.npmjs.org"
  },
  "repository": "git@github.com:molgenis/molgenis-js-library-example.git",
  "keywords": [
    "molgenis"
  ],
  "author": "MOLGENIS Team, System Genetics, UMCG, Netherlands",
  "license": "LGPL-3.0",
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-eslint": "^8.2.6",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-stage-2": "^6.16.0",
    "babel-register": "^6.16.3",
    "babel-runtime": "^6.26.0",
    "chai": "^4.0.0-canary.1",
    "cross-env": "^5.2.0",
    "del": "^2.2.2",
    "eslint": "^4.15.0",
    "eslint-plugin-import": "^2.2.0",
    "istanbul": "1.1.0-alpha.1",
    "mocha": "^3.1.2",
    "rollup": "^0.64.1",
    "rollup-plugin-babel": "^2.6.1",
    "rollup-plugin-istanbul": "^2.0.1"
  },
  "babel": {
    "presets": [
      "latest",
      "stage-2"
    ],
    "plugins": [
      "transform-runtime"
    ]
  }
}
